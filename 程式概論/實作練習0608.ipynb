   "source": [
    "from datetime import *\n",
    "#1\n",
    "bd=date(1999,9,10)\n",
    "print(bd)\n",
    "#2\n",
    "td=date.today()\n",
    "print(td)\n",
    "#3\n",
    "diff=td-bd         \n",
    "print(diff.days)\n",
    "living_years=diff.days//365\n",
    "living_days=diff.days%365\n",
    "print(\"到今天為止已生活了\"+str(living_years)+\"年又\"+str(living_days)+\"天\")"
   ]
